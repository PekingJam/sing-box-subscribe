{
  "log": {
    "level": "warn",
    "timestamp": false
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:32590",
      "external_ui": "ui",
      "secret": "orange123",
      "external_ui_download_url": "https://re.dandaner.cn/https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false,
      "store_rdrc": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "cloudflare",
        "type": "tls",
        "server": "1.1.1.1",
        "detour": "境外出口"
      },
      {
        "tag": "tx-udp",
        "type": "udp",
        "server": "180.184.1.1",
        "detour": "境内出口"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "tx-udp"
      },
      {
        "clash_mode": "global",
        "server": "cloudflare"
      },
      {
        "domain": [
          "drive.dandaner.cn",
          "oss.dandaner.cn"
        ],
        "server": "tx-udp"
      },
      {
        "rule_set": [
          "cn_site",
          "private_site"
        ],
        "server": "tx-udp"
      }
    ],
    "strategy": "ipv4_only",
    "reverse_mapping": true,
    "final": "cloudflare",
    "independent_cache": false
  },
  "inbounds": [
    {
      "tag": "tun_in",
      "type": "tun",
      "interface_name": "tnu6",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "stack": "system",
      "strict_route": true,
      "auto_route": true,
      "exclude_package": [
        "com.xiaomi.market",
        "com.tencent.mm",
        "com.eg.android.AlipayGphone",
        "com.ss.android.ugc.aweme",
        "com.jingdong.app.mall",
        "com.taobao.taobao",
        "com.taobao.idlefish",
        "com.sankuai.meituan",
        "com.xunmeng.pinduoduo",
        "com.cmbchina.ccd.pluto.cmbActivity",
        "com.tencent.mobileqq",
        "com.miui.carlink"
      ]
    },
    {
      "tag": "mixed_in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 32550
    }
  ],
  "outbounds": [
    {
      "tag": "境外出口",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
