{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://drive.dandaner.cn/d/sub/singbox/dashboard/yacd.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "detour": "\uD83C\uDF0D 境外出口"
      },
      {
        "tag": "tx",
        "address": "223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tx",
        "disable_cache": true
      },
      {
        "domain": [
          "cloudapi.bytedance.net"
        ],
        "server": "block"
      },
      {
        "rule_set": "google_site",
        "server": "google"
      },
      {
        "rule_set": "cn_site",
        "server": "tx"
      },
      {
        "clash_mode": "direct",
        "server": "tx"
      },
      {
        "clash_mode": "global",
        "server": "google"
      }
    ],
    "reverse_mapping": true,
    "final": "google",
    "strategy": "ipv4_only",
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "interface_name": "tnu0",
      "inet4_address": "172.19.0.1/30",
      "stack": "system",
      "strict_route": true,
      "auto_route": true,
      "auto_redirect": false,
      "sniff_override_destination": false,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag": "🌍 境外出口",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "\uD83C\uDDE8\uD83C\uDDF3|\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "tag": "\uD83C\uDDE8\uD83C\uDDF3 境内出口",
      "type": "selector",
      "outbounds": [
        "direct",
        "\uD83C\uDF0D 境外出口",
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇨🇳"
          ]
        }
      ]
    },
    {
      "tag": "✈️ Telegram",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "\uD83C\uDDE8\uD83C\uDDF3|\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "tag": "\uD83C\uDFB6 Tiktok",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "\uD83C\uDDE8\uD83C\uDDF3|\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "tag": "🎈 订阅信息",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "🌍 境外出口",
    "rules": [
      {
        "port": 21114,
        "outbound": "block"
      },
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "🌍 境外出口"
      },
