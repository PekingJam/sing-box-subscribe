      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "10.10.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "detour": "🌍 境外出口"
      },
      {
        "tag": "tx",
        "address": "223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tx",
        "disable_cache": true
      },
      {
        "domain": [
          "cloudapi.bytedance.net"
        ],
        "server": "block"
      },
      {
        "rule_set": "google_site",
        "server": "google"
      },
      {
        "rule_set": "cn_site",
        "server": "tx"
      },
      {
        "clash_mode": "direct",
        "server": "tx"
      },
      {
        "clash_mode": "global",
        "server": "google"
      },
      {
        "inbound": "tun",
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "ncn_site",
        "server": "google"
      }
    ],
    "reverse_mapping": true,
    "final": "google",
    "strategy": "ipv4_only",
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "stack": "mixed",
      "strict_route": true,
      "auto_route": true,
      "sniff_override_destination": false,
      "sniff": true,
      "platform": {
        "http_proxy": {
          "enabled": false,
          "server": "127.0.0.1",
          "server_port": 32580
        }
      }
    },
    {
      "tag": "mixed",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 32580,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag": "🌍 境外出口",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "\uD83C\uDDE8\uD83C\uDDF3|\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "tag": "\uD83D\uDCE2 社交通讯",
