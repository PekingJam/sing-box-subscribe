          ]
        }
      ]
    },
    {
      "tag": "🇨🇳 境内出口",
      "type": "selector",
      "outbounds": [
        "{all}",
        "direct"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "\uD83C\uDDE8\uD83C\uDDF3"
          ]
        }
      ]
    },
    {
      "tag": "🎈 订阅信息",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "\uD83D\uDFE2"
          ]
        }
      ]
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
